#-------------------------------------------------------------
# Bash Aliases
#-------------------------------------------------------------
alias e='exit'
alias c='clear'
alias ld='ls -d'
alias ll='ls -lh'
alias cl='clear && ls'
alias lsm='ls *.md'
alias pls='sudo $(history -p \!\!)'
alias grep='/usr/bin/egrep'
export PS1="_____________________ macOS | \w \n$ "
